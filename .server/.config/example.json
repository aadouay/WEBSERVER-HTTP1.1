{
  "servers": [
    {
      "port": "3000",
      "name": "my-app",
      "version": "0.1.0",
      "404": "./404.html",
      "500": "./500.html",
      "log": "./access.log",
      "bodylimit": "1048576",
      "timeout": "30000",
      "uploaddir": "./downloads",
      "index": "index.html",
      "root": "./",
      "routes": [
        {
          "path": "/delete/1",
          "source": "/delete/test1.mp4",
          "method": ["GET", "DELETE"]
        },
        {
          "path": "/delete/2",
          "source": "/delete/test2.html",
          "method": ["GET", "DELETE"]
        }
      ]
    }
  ]
}