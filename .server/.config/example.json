{
  "servers": [
    {
      "port": "3000",
      "name": "my-app",
      "version": "0.1.0",
      "notfound": "./404.html",
      "servererror": "./500.html",
      "log": "./access.log",
      "bodylimit": "1048576",
      "timeout": "30000",
      "uploaddir": "./downloads",
      "index": "./index.html",
      "root": "./",
      "routes": [
        {
          "path": "/",
          "source": "./index.html",
          "method": ["GET"]
        }
      ]
    }
  ]
}